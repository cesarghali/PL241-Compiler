# ekin's program


main

var a, b, c, g, d, e, z, y, t, u;

{
	let a <- 1;
	let b <- 0;
	let c <- 0;
	while a < 10 do
		if (a / 2) == 0 then
			let c <- 1;
			let b <- b - 1
		else
			let c <- 2;
			while c < 5 do
				let b <- b + 1;
				let c <- c + 1
			od
		fi;
		let a <- a + 1
	od;
	call OutputNum((a + b))
}.